echo "example 1: "

make && ./sim_main tests/example1.img 43  > temp.out  && diff temp.out results/example1.txt | grep -v "Loading memory image"

echo "example 1 -s"
 ./sim_main tests/example1.img 43 -s  > temp.out  && diff temp.out results/example1-s.txt | grep -v "Loading memory image"

echo "example 1 -f"
 ./sim_main tests/example1.img 43 -f  > temp.out  && diff temp.out results/example1-f.txt | grep -v "Loading memory image"

